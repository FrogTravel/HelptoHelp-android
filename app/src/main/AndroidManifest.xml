<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="snu.kr.helptohelp">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="snu.kr.helptohelp.activities.search.SearchResultView"/>
        <activity android:name="snu.kr.helptohelp.activities.filterSearch.FilterSearchView"/>
        <activity android:name=".activities.userPage.UserPageView"/>
        <activity android:name=".activities.profile.UserProfileView"/>
    </application>

</manifest>